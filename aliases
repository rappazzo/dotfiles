alias gitk="gitk --all --since=\"9 months ago\""
alias rimshot='curl http://soundboard.local/play/rimshot > /dev/null 2>&1'
alias sneeze='curl http://soundboard.local/sneeze > /dev/null 2>&1'
alias kwong='curl http://soundboard.local/play/kwong2 > /dev/null 2>&1'
alias previous-git-version='ls /usr/local/Cellar/git | sort -n | tail -n 2 | head -n 1'
alias latest-git-version='ls /usr/local/Cellar/git | sort -n | tail -n 1'
alias flush-dns="dscacheutil -flushcache; killall -HUP mDNSResponder"
alias win64="VBoxManage startvm Win64"
alias m2settings="mvim ~/.m2/settings.xml"
alias tcmigrate="modules/main/tcard/etc/migration/flyway-commandline/tc-migrate.sh migrate"
alias file-tree="find . -print | sed -e 's;[^/]*/;├────;g;s;├────; |;g'"
alias git-conflicts="git ls-files --unmerged | awk '{print $4}' | sort -u | wc -l"

